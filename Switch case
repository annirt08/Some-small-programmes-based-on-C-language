#include<stdio.h>
#include<math.h>
main(){
printf("pick a what you want:\n 0.Book \n 1.Newspaper\n");
int choice;
scanf("%d",&choice);
switch(choice)
{
	case 0: 
	printf("your choice is Book");
	break;
case 1: 
printf("your choice is Newspaper");
break;
default: 
printf("Please select correct option");
}
}
